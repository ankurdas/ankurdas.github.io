    <!-- Portfolio Modals -->
    <div class="portfolio-modal modal fade" id="swingarm" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-content">
            <div class="close-modal" data-dismiss="modal">
                <div class="lr">
                    <div class="rl">
